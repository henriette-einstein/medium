{
  "language": "en",
  "searchLimit": 10,
  "thumbnailWidth": 250,
  "defaultNotePath": "/",
  "templates": [
    {
      "name": "Default",
      "templateString": "[{title}]({url})",
      "createNote": false,
      "customPath": "",
      "useTemplateFile": false,
      "templateFilePath": ""
    },
    {
      "name": "Intro",
      "templateString": "[{title}]({url})\n{intro}",
      "createNote": false,
      "customPath": "",
      "useTemplateFile": false,
      "templateFilePath": ""
    },
    {
      "name": "Full",
      "templateString": "{thumbnail}\n# {title} \n{intro}\n# See also\n- [{title} - Wikipedia]({url})\n",
      "createNote": false,
      "customPath": "",
      "useTemplateFile": false,
      "templateFilePath": ""
    },
    {
      "name": "Weblink Note",
      "templateString": "---\ntype: \"[[Weblink]]\"\ncollections: \"[[Wikipedia]]\"\ncreated: \"{{date}}\"\ntitle: \"{title}\"\nurl: \"{url}\"\nthumbnail: \"{thumbnail}\"\n---\n{thumbnail}\n{description}\n## See\n- [{title}]({url})",
      "createNote": true,
      "customPath": "40 Ressources/Weblinks/Wikipedia",
      "useTemplateFile": false,
      "templateFilePath": ""
    }
  ],
  "placeCursorInfrontOfInsert": false,
  "autoInsertSingleResponseQueries": false,
  "prioritizeArticleTitle": false,
  "cleanupIntros": true,
  "openArticleInFullscreen": false,
  "openArticlesInBrowser": false,
  "openCreatedNotes": false,
  "overrideFiles": false,
  "showedSurfingMessage": true
}